<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- 니 최상위 도메인 직속 페이지들 (메인 페이지는 빼고, 기존 상품 페이지들만!) 지도 -->
  <sitemap>
    <loc>https://rkskqdl-a11y.github.io/sitemap_main.xml</loc>
    <lastmod>{{ "now" | date: "%Y-%m-%dT%H:%M:%S%z" }}</lastmod> <!-- sitemap_main.xml이 마지막으로 수정된 시점 -->
  </sitemap>
  <!-- content-manager 레포에서 생성되는 새 광고 페이지들을 위한 지도 -->
  <sitemap>
    <loc>https://rkskqdl-a11y.github.io/content-manager/sitemap.xml</loc>
    <lastmod>{{ "now" | date: "%Y-%m-%dT%H:%M:%S%z" }}</lastmod> <!-- content-manager의 sitemap이 마지막으로 생성된 시점 -->
  </sitemap>
</sitemapindex>
